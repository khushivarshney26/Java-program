import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
            String k=sc.nextLine();
            for(int i=0;i<k.length();i++){
            if( k.charAt(i)=='a'|| k.charAt(i)=='e' || k.charAt(i)=='i' || k.charAt(i)=='o' || k.charAt(i)=='u' ){
                System.out.print(i + " ");
            }

            }
}
}
