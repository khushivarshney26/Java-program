import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        int n=a.length();
        String b[]=a.split(" ");
        String t="";
        for(String c:b){
            if(c.length()<n){
                n=c.length();
                t=c;
            }
        }
        System.out.println(t);
    }
}
